//非负整数快速读入
const int MaxLen=1<<15;  
char buffer[MaxLen],*S,*T;  
inline char Get_Char()  
{  
    if(S==T)  
    {  
        T=(S=buffer)+fread(buffer,1,MaxLen,stdin);  
        if(S==T) return EOF;  
    }  
    return *S++;  
}
inline int Get_Int()
{  
    char c;  
    int re=0;  
    for(c=Get_Char();c<'0'||c>'9';c=Get_Char());  
    while(c>='0'&&c<='9')  
        re=(re<<1)+(re<<3)+(c-'0'),c=Get_Char();  
    return re;  
}
